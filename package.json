{
  "name": "rn-skia-progress",
  "version": "0.1.0",
  "description": "React Native library which allows the use of high quality progress components with Skia",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "prepare": "bob build"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "keywords": [
    "react-native",
    "react-native-skia",
    "progress",
    "progress-circle",
    "progress-bar",
    "animation",
    "gesture",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sworzen1/rn-skia-progress.git"
  },
  "author": "Bartłomiej Sworzeń <b.sworzen@gmail.com> (https://github.com/Sworzen1)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sworzen1/rn-skia-progress/issues"
  },
  "homepage": "https://github.com/Sworzen1/rn-skia-progress#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/react": "^18.2.44",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-builder-bob": "^0.20.0",
    "typescript": "^5.5.3"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "@shopify/react-native-skia": "^1.0.0",
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "^2.0.0",
    "react-native-reanimated": "^2.0.0 || ^3.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
